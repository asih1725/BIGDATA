    "\n",
    "columns2 = [\"nama\", \"departemen\", \"gaji\", \"usia\"]\n",
    "df2 = spark.createDataFrame(data2, columns2)\n",
    "df2.createOrReplaceTempView(\"karyawan2\")\n",
    "\n",
    "# === SQL: Cari rata-rata gaji per kelompok usia (misal: usia <30 dan >=30) ===\n",
    "hasil2 = spark.sql(\"\"\"\n",
    "SELECT\n",
    "    CASE WHEN usia < 30 THEN 'Muda (<30)'\n",
    "         ELSE 'Dewasa (>=30)' END AS kelompok_usia,\n",
    "    AVG(gaji) AS rata_gaji\n",
    "FROM karyawan2\n",
    "GROUP BY CASE WHEN usia < 30 THEN 'Muda (<30)'\n",
    "              ELSE 'Dewasa (>=30)' END\n",
    "\"\"\")\n",
    "\n",
    "hasil2.show()\n",
    "\n",
    "# === Karyawan dengan gaji di atas rata-rata dalam kelompoknya ===\n",
    "hasil3 = spark.sql(\"\"\"\n",
    "WITH avg_per_kelompok AS (\n",
    "    SELECT\n",
    "        CASE WHEN usia < 30 THEN 'Muda (<30)'\n",
    "             ELSE 'Dewasa (>=30)' END AS kelompok_usia,\n",
    "        AVG(gaji) AS rata_gaji\n",
    "    FROM karyawan2\n",
    "    GROUP BY CASE WHEN usia < 30 THEN 'Muda (<30)'\n",
    "                  ELSE 'Dewasa (>=30)' END\n",
    ")\n",
    "SELECT \n",
    "    k.nama, k.departemen, k.gaji, k.usia, a.kelompok_usia\n",
    "FROM karyawan2 k\n",
    "JOIN avg_per_kelompok a\n",
    "  ON (CASE WHEN k.usia < 30 THEN 'Muda (<30)' ELSE 'Dewasa (>=30)' END) = a.kelompok_usia\n",
    "WHERE k.gaji > a.rata_gaji\n",
    "\"\"\")\n",
    "\n",
    "pdf3 = hasil3.toPandas()\n",
    "\n",
    "# === Visualisasi ===\n",
    "pdf3['kelompok_usia'].value_counts().plot(kind='pie', autopct='%1.1f%%')\n",
    "plt.title(\"Distribusi Karyawan dengan Gaji di Atas Rata-Rata\")\n",
    "plt.ylabel(\"\")\n",
    "plt.show()\n"
