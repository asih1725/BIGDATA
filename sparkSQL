    "]\n",
    "\n",
    "columns_karyawan = [\"Nama\", \"Departemen\", \"Gaji\", \"Usia\", \"DeptId\"]\n",
    "df_karyawan = spark.createDataFrame(data_karyawan, columns_karyawan)\n",
    "df_karyawan.createOrReplaceTempView(\"karyawan\")\n",
    "\n",
    "# 3Ô∏è‚É£ Dataset referensi: Departemen\n",
    "data_departemen = [\n",
    "    (1, \"HR\", \"Yogyakarta\"),\n",
    "    (2, \"Finance\", \"Jakarta\"),\n",
    "    (3, \"IT\", \"Bandung\"),\n",
    "    (4, \"Marketing\", \"Surabaya\"),\n",
    "    (5, \"Sales\", \"Semarang\")\n",
    "]\n",
    "df_departemen = spark.createDataFrame(data_departemen, [\"DeptId\", \"DeptName\", \"Lokasi\"])\n",
    "df_departemen.createOrReplaceTempView(\"departemen\")\n",
    "\n",
    "# 4Ô∏è‚É£ Analisis SQL: SUM(), AVG(), COUNT(), JOIN\n",
    "hasil_analisis = spark.sql(\"\"\"\n",
    "    SELECT k.Departemen, d.Lokasi,\n",
    "           COUNT(*) AS JumlahKaryawan,\n",
    "           SUM(k.Gaji) AS TotalGaji,\n",
    "           ROUND(AVG(k.Gaji),2) AS RataRataGaji\n",
    "    FROM karyawan k\n",
    "    JOIN departemen d ON k.DeptId = d.DeptId\n",
    "    GROUP BY k.Departemen, d.Lokasi\n",
    "    ORDER BY TotalGaji DESC\n",
    "\"\"\")\n",
    "\n",
    "hasil_analisis.show()\n",
    "\n",
    "# 5Ô∏è‚É£ Visualisasi: Total Gaji per Departemen\n",
    "pdf = hasil_analisis.toPandas()\n",
    "plt.figure(figsize=(8,5))\n",
    "plt.bar(pdf['Departemen'], pdf['TotalGaji'], color='orange')\n",
    "plt.title(\"Total Gaji per Departemen (100 Data Karyawan)\")\n",
    "plt.xlabel(\"Departemen\")\n",
    "plt.ylabel(\"Total Gaji\")\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
