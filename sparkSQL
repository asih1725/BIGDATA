   "source": [
    "from pyspark.sql import SparkSession\n",
    "\n",
    "# Membuat SparkSession\n",
    "spark = SparkSession.builder \\\n",
    "    .appName(\"Advanced SQL Tasks\") \\\n",
    "    .getOrCreate()\n",
    "\n",
    "# (Opsional) Sembunyikan log yang berlebihan\n",
    "spark.sparkContext.setLogLevel(\"ERROR\")\n",
    "\n",
    "# Data contoh\n",
    "data = [\n",
    "    ('James', 34, 'M', 3000, 1),\n",
    "    ('Anna', 28, 'F', 4100, 2),\n",
    "    ('Lee', 23, 'M', 2700, 1),\n",
    "    ('Linda', 28, 'F', 3900, 2),\n",
    "    ('John', 34, 'M', 3500, 1),\n",
    "    ('Sofia', 23, 'F', 3200, 3),\n",
    "    ('Kevin', 30, 'M', 4100, 2)\n",
    "]\n",
    "\n",
    "# Nama kolom\n",
    "columns = ['Name', 'Age', 'Gender', 'Salary', 'DeptId']\n",
    "\n",
    "# Membuat DataFrame\n",
    "df = spark.createDataFrame(data, schema=columns)\n",
    "\n",
    "# Membuat temporary view\n",
    "df.createOrReplaceTempView(\"employees\")\n",
    "\n",
    "# ======================================================\n",
    "# üß† 1. Cari rata-rata gaji per departemen\n",
    "# ======================================================\n",
    "print(\"=== 1Ô∏è‚É£ Rata-rata Gaji per Departemen ===\")\n",
    "spark.sql('''\n",
    "SELECT DeptId,\n",
    "       ROUND(AVG(Salary), 2) AS AvgSalary\n",
    "FROM employees\n",
    "GROUP BY DeptId\n",
    "ORDER BY DeptId\n",
    "''').show()\n",
    "\n",
    "# ======================================================\n",
    "# üß† 2. Temukan karyawan dengan gaji di atas rata-rata gender masing-masing\n",
    "# ======================================================\n",
    "print(\"=== 2Ô∏è‚É£ Karyawan dengan Gaji di Atas Rata-rata Gender ===\")\n",
    "spark.sql('''\n",
    "SELECT Name, Gender, Salary\n",
    "FROM employees e\n",
    "WHERE Salary > (\n",
    "    SELECT AVG(Salary)\n",
    "    FROM employees\n",
    "    WHERE Gender = e.Gender\n",
    ")\n",
    "ORDER BY Gender, Salary DESC\n",
    "''').show()\n",
    "\n",
    "# ======================================================\n",
    "# üß† 3. Buat ranking karyawan berdasarkan gaji dalam departemen mereka\n",
    "# ======================================================\n",
    "print(\"=== 3Ô∏è‚É£ Ranking Gaji per Departemen ===\")\n",
    "spark.sql('''\n",
    "SELECT Name, DeptId, Salary,\n",
    "       RANK() OVER (PARTITION BY DeptId ORDER BY Salary DESC) AS RankInDept\n",
    "FROM employees\n",
    "ORDER BY DeptId, RankInDept\n",
    "''').show()\n"
   ]
  },
