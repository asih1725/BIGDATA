    "# === Tugas 7: Analisis Tren Gaji dan Visualisasi ===\n",
    "from pyspark.sql import SparkSession\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "# Membuat SparkSession (jika belum ada)\n",
    "spark = SparkSession.builder.appName(\"Pertemuan9_Tugas7\").getOrCreate()\n",
    "\n",
    "# Dataset contoh\n",
    "employees = spark.createDataFrame([\n",
    "    ('James', 34, 'M', 3000, 1),\n",
    "    ('Anna', 28, 'F', 4100, 2),\n",
    "    ('Lee', 23, 'M', 2700, 1),\n",
    "    ('Maria', 29, 'F', 3800, 2),\n",
    "    ('John', 31, 'M', 3300, 1),\n",
    "    ('Siti', 27, 'F', 4200, 2),\n",
    "    ('Andi', 25, 'M', 2900, 1),\n",
    "    ('Rina', 35, 'F', 4400, 2)\n",
    "], ['Name', 'Age', 'Gender', 'Salary', 'DeptId'])\n",
    "\n",
    "# Registrasi tabel ke Spark SQL\n",
    "employees.createOrReplaceTempView(\"employees\")\n",
    "\n",
    "# === Analisis tren gaji per usia ===\n",
    "salary_trends = spark.sql('''\n",
    "SELECT \n",
    "    Age, \n",
    "    ROUND(AVG(Salary), 2) AS AverageSalary\n",
    "FROM employees\n",
    "GROUP BY Age\n",
    "ORDER BY Age\n",
    "''').toPandas()\n",
    "\n",
    "# === Visualisasi menggunakan matplotlib ===\n",
    "plt.figure(figsize=(10, 5))\n",
    "plt.plot(\n",
    "    salary_trends['Age'], \n",
    "    salary_trends['AverageSalary'], \n",
    "    marker='o', \n",
    "    linestyle='-', \n",
    "    color='b'\n",
    ")\n",
    "plt.xlabel('Usia')\n",
    "plt.ylabel('Rata-rata Gaji')\n",
    "plt.title('Tren Rata-rata Gaji Berdasarkan Usia')\n",
    "plt.grid(True)\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
