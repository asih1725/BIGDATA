      "+-----+---+---------+-----------+------+-------------+--------------+\n",
      "| Name|Age| DeptName|ProjectName|Salary|AvgDeptSalary|SalaryCategory|\n",
      "+-----+---+---------+-----------+------+-------------+--------------+\n",
      "| John| 31|       HR|  Project A|  3300|       3000.0| Above Average|\n",
      "| John| 31|       HR|  Project B|  3300|       3000.0| Above Average|\n",
      "|James| 34|       HR|  Project A|  3000|       3000.0| Below Average|\n",
      "|James| 34|       HR|  Project B|  3000|       3000.0| Below Average|\n",
      "|  Lee| 23|       HR|  Project A|  2700|       3000.0| Below Average|\n",
      "|  Lee| 23|       HR|  Project B|  2700|       3000.0| Below Average|\n",
      "| Anna| 28|Marketing|  Project C|  4100|       3950.0| Above Average|\n",
      "|Maria| 29|Marketing|  Project C|  3800|       3950.0| Below Average|\n",
      "+-----+---+---------+-----------+------+-------------+--------------+\n",
      "\n"
     ]
    }
   ],
   "source": [
    "from pyspark.sql import SparkSession\n",
    "\n",
    "# Membuat SparkSession\n",
    "spark = SparkSession.builder.appName('Pertemuan9').getOrCreate()\n",
    "\n",
    "# === Dataset untuk demonstrasi ===\n",
    "employees = spark.createDataFrame([\n",
    "    ('James', 34, 'M', 3000, 1),\n",
    "    ('Anna', 28, 'F', 4100, 2),\n",
    "    ('Lee', 23, 'M', 2700, 1),\n",
    "    ('Maria', 29, 'F', 3800, 2),\n",
    "    ('John', 31, 'M', 3300, 1)\n",
    "], ['Name', 'Age', 'Gender', 'Salary', 'DeptId'])\n",
    "\n",
    "departments = spark.createDataFrame([\n",
    "    (1, 'HR'),\n",
    "    (2, 'Marketing')\n",
    "], ['DeptId', 'DeptName'])\n",
    "\n",
    "projects = spark.createDataFrame([\n",
    "    (1, 'Project A'),\n",
    "    (1, 'Project B'),\n",
    "    (2, 'Project C')\n",
    "], ['DeptId', 'ProjectName'])\n",
    "\n",
    "# Mendaftarkan tabel ke Spark SQL\n",
    "employees.createOrReplaceTempView('employees')\n",
    "departments.createOrReplaceTempView('departments')\n",
    "projects.createOrReplaceTempView('projects')\n",
    "\n",
    "# === Tugas 4: Multi-level joins & Subqueries ===\n",
    "result = spark.sql(\"\"\"\n",
    "SELECT \n",
    "    e.Name,\n",
    "    e.Age,\n",
    "    d.DeptName,\n",
    "    p.ProjectName,\n",
    "    e.Salary,\n",
    "    (SELECT AVG(Salary) \n",
    "     FROM employees e2 \n",
    "     WHERE e2.DeptId = e.DeptId) AS AvgDeptSalary,\n",
    "    CASE \n",
    "        WHEN e.Salary > (SELECT AVG(Salary) FROM employees e3 WHERE e3.DeptId = e.DeptId)\n",
    "        THEN 'Above Average'\n",
    "        ELSE 'Below Average'\n",
    "    END AS SalaryCategory\n",
    "FROM employees e\n",
    "JOIN departments d ON e.DeptId = d.DeptId\n",
    "JOIN projects p ON e.DeptId = p.DeptId\n",
    "ORDER BY d.DeptName, e.Salary DESC\n",
    "\"\"\")\n",
    "\n",
    "result.show()\n"
   ]
  },
