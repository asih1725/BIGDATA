   ],
   "source": [
    "from pyspark.sql import SparkSession\n",
    "\n",
    "# Membuat SparkSession (kalau belum ada)\n",
    "spark = SparkSession.builder.appName('Pertemuan9_Tugas5_6').getOrCreate()\n",
    "\n",
    "# Dataset contoh\n",
    "employees = spark.createDataFrame([\n",
    "    ('James', 34, 'M', 3000, 1),\n",
    "    ('Anna', 28, 'F', 4100, 2),\n",
    "    ('Lee', 23, 'M', 2700, 1),\n",
    "    ('Maria', 29, 'F', 3800, 2),\n",
    "    ('John', 31, 'M', 3300, 1),\n",
    "    ('Siti', 27, 'F', 4200, 2)\n",
    "], ['Name', 'Age', 'Gender', 'Salary', 'DeptId'])\n",
    "\n",
    "departments = spark.createDataFrame([\n",
    "    (1, 'HR'),\n",
    "    (2, 'Marketing')\n",
    "], ['DeptId', 'DeptName'])\n",
    "\n",
    "# Registrasi tabel ke Spark SQL\n",
    "employees.createOrReplaceTempView('employees')\n",
    "departments.createOrReplaceTempView('departments')\n",
    "\n",
    "# === üíº TUGAS 5 ===\n",
    "# Rata-rata gaji per departemen + Ranking karyawan berdasarkan gaji\n",
    "spark.sql(\"\"\"\n",
    "SELECT \n",
    "    d.DeptName,\n",
    "    e.Name,\n",
    "    e.Salary,\n",
    "    AVG(e.Salary) OVER (PARTITION BY e.DeptId) AS AvgDeptSalary,\n",
    "    RANK() OVER (PARTITION BY e.DeptId ORDER BY e.Salary DESC) AS SalaryRank\n",
    "FROM employees e\n",
    "JOIN departments d ON e.DeptId = d.DeptId\n",
    "ORDER BY d.DeptName, SalaryRank\n",
    "\"\"\").show()\n"
   ]
  },
