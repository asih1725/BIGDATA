   "source": [
    "from pyspark.sql import SparkSession\n",
    "\n",
    "# Membuat SparkSession\n",
    "spark = SparkSession.builder.appName(\"WindowFunctionsExample\").getOrCreate()\n",
    "\n",
    "# Data contoh\n",
    "data = [\n",
    "    ('James', 34, 'M', 3000, 1),\n",
    "    ('Anna', 28, 'F', 4100, 2),\n",
    "    ('Lee', 23, 'M', 2700, 1),\n",
    "    ('Maria', 34, 'F', 3200, 2),\n",
    "    ('John', 28, 'M', 3600, 2),\n",
    "    ('Siti', 23, 'F', 2800, 1)\n",
    "]\n",
    "columns = ['Name', 'Age', 'Gender', 'Salary', 'DeptId']\n",
    "\n",
    "# Membuat DataFrame\n",
    "df = spark.createDataFrame(data, schema=columns)\n",
    "df.createOrReplaceTempView(\"employees\")\n",
    "\n",
    "# --- Tugas 3: Window Function ---\n",
    "# Mencari peringkat karyawan berdasarkan gaji dalam kelompok umur\n",
    "spark.sql('''\n",
    "SELECT \n",
    "    Name, \n",
    "    Age, \n",
    "    Salary, \n",
    "    ROW_NUMBER() OVER (PARTITION BY Age ORDER BY Salary DESC) AS rank\n",
    "FROM employees\n",
    "''').show()\n",
    "\n",
    "# Jika kamu hanya ingin top 3 saja per kelompok umur:\n",
    "spark.sql('''\n",
    "SELECT *\n",
    "FROM (\n",
    "    SELECT \n",
    "        Name, \n",
    "        Age, \n",
    "        Salary, \n",
    "        ROW_NUMBER() OVER (PARTITION BY Age ORDER BY Salary DESC) AS rank\n",
    "    FROM employees\n",
    ")\n",
    "WHERE rank <= 3\n",
    "''').show()\n"
   ]
  },
